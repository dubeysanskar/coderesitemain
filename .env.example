# .env (example template)

# === Google & Gemini APIs ===
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_CX=your_google_custom_search_engine_id

GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CX=your_google_custom_search_engine_id

# === Google Sheets Service Account ===
GOOGLE_SHEETS_CREDENTIALS={
  "type": "service_account",
  "project_id": "your_project_id",
  "private_key_id": "your_private_key_id",
  "private_key": "-----BEGIN PRIVATE KEY-----\\nyour_private_key_here\\n-----END PRIVATE KEY-----\\n",
  "client_email": "your_service_account_email@your_project.iam.gserviceaccount.com",
  "client_id": "your_client_id",
  "auth_uri": "https://accounts.google.com/o/oauth2/auth",
  "token_uri": "https://oauth2.googleapis.com/token",
  "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
  "client_x509_cert_url": "https://www.googleapis.com/robot/v1/metadata/x509/your_service_account_email@your_project.iam.gserviceaccount.com",
  "universe_domain": "googleapis.com"
}

# === Gemini-specific keys ===
GEMINI_RESUME_API_KEY=your_gemini_resume_api_key
GEMINI_REPORT_API_KEY=your_gemini_report_api_key
VITE_GEMINI_REPORT_API_KEY=your_vite_gemini_report_api_key
VITE_GEMINI_LEAD_API_KEY=your_vite_gemini_lead_api_key

# === Brevo (Email) API ===
VITE_BREVO_API_KEY=your_brevo_api_key
BREVO_API_KEY=your_server_side_brevo_api_key

# === Clerk (Authentication) ===
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# === Web3Forms ===
VITE_WEB3FORMS_ACCESS_KEY=your_web3forms_access_key

# === Builder.io ===
VITE_BUILDER_API=your_builder_api_key
